<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="Title" value="GGMeeting视频会议系统 2015"/>
    
    <!--服务器IP -->
    <add key="ServerIP" value="127.0.0.1"/>
    <add key="ServerPort" value="4538"/>
    <add key="OmcsPort" value="4534"/>

    <!-- P2PDisabled , P2PChannelFirst ,AutoChooseFaster ,AutoChooseFaster4Audio-->
    <add key="ChannelMode" value="P2PDisabled"/>
    <add key="SpeakerIndex" value="0"/>
    <add key="MicrophoneIndex" value="0"/>
    <!--摄像头采集分辨率的大小 [160,120] [320,240] [640,480]-->
    <add key="CameraVideoSize" value="320,240"/> 

    <!--UserID的最大长度-->
    <add key="MaxLengthOfUserID" value="20"/>
  </appSettings>
</configuration>
